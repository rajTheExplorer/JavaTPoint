class PalindromeNumber
{
	public static void main(String[] args) {
		int num=1232,rem,sum=0;
		int temp=num;
		while(num>0)
		{
			rem=num%10;
			sum=sum*10+rem;
			num=num/10;
		}
		if(sum==temp)
		{
			System.out.println("its a palindrome");
		}
		else
		{
			System.out.println("its not a palindrome");
		}
	}
}
